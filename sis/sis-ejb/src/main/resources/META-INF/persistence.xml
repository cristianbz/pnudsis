<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="sisPU" transaction-type="JTA">
<provider>org.hibernate.ejb.HibernatePersistence</provider>
<jta-data-source>java:jboss/datasources/sisDS</jta-data-source>
<class>ec.gob.ambiente.sigma.model.ActionPlans</class>
<class>ec.gob.ambiente.sigma.model.Partners</class>
<class>ec.gob.ambiente.sigma.model.Projects</class>
<class>ec.gob.ambiente.sigma.model.ProjectsCobenefits</class>
<class>ec.gob.ambiente.sigma.model.ProjectsSafeguards</class>
<class>ec.gob.ambiente.sigma.model.Safeguards</class>
<class>ec.gob.ambiente.sis.model.AdvanceExecutionSafeguards</class>
<class>ec.gob.ambiente.sis.model.AdvanceSectors</class>
<class>ec.gob.ambiente.sis.model.Answers</class>
<class>ec.gob.ambiente.sis.model.ExecutiveSummaries</class>
<class>ec.gob.ambiente.sis.model.Questions</class>
<class>ec.gob.ambiente.sis.model.QuestionsAnswers</class>
<class>ec.gob.ambiente.sis.model.Sectors</class>
<class>ec.gob.ambiente.sis.model.TableResponses</class>
<class>ec.gob.ambiente.sis.model.ValueAnswers</class>
<class>ec.gob.ambiente.suia.model.Users</class>
<properties>
<property name="showSql" value="true"/>
<property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect"/>
<!--   <property name="hibernate.dialect" value="org.hibernate.dialect.SQLServer2012Dialect" /> -->
<!--    <property name="hibernate.dialect" value="org.hibernate.dialect.SQLServerDialect" />  -->
<property name="hibernate.format_sql" value="true"/>
<property name="hibernate.show_sql" value="false"/>
<property name="hibernate.generate_statistics" value="false"/>
<property name="hibernate.temp.use_jdbc_metadata_defaults" value="false"/>
</properties>
</persistence-unit>
</persistence>